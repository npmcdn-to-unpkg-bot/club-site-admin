{{!-- see: https://jsfiddle.net/ann7tctp/ --}}
<h2>Past Results</h2>
<div class="list-group list-group-root well">
  {{#each data.menu}}
    <a href="#item-{{this.year}}" class="list-group-item" data-toggle="collapse">
        <i class="glyphicon glyphicon-chevron-right"></i><strong>{{this.year}}</strong>
    </a>
    <div class="list-group collapse" id="item-{{this.year}}">
        {{#each this.months}}
            <a href="#" class="list-group-item">{{this.monthName}}</a>
        {{/each}}
    </div>
  {{/each}}
</div>
